  "dependencies": {
    "aws-sdk": "^2.315.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.5",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.2",
    "awesome-typescript-loader": "^5.0.0",
    "aws-sdk": "^2.315.0",
    "aws-sdk-mock": "^3.2.0",
    "copy-webpack-plugin": "^4.4.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^3.1.2",
    "handlebars": "^4.7.3",
    "jest": "^23.6.0",
    "prettier": "^1.19.1",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.1.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.0.7",
    "webpack-node-externals": "^1.7.2",
    "yarn": "^1.9.4"
  }
