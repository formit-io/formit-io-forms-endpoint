{
  "extends": "tslint-config-airbnb",
  "rules": {
    "max-line-length": [
      true,
      160
    ],
    "trailing-comma": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "import-name": false,
    "align": false,
    "semicolon": false
  }
}
